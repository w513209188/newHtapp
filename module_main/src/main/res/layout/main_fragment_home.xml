<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.wb.baselib.view.TopBarView
        android:id="@+id/topbarview"
        app:leftType="imageButton"
        app:leftImageResource="@drawable/public_back_hs"
        app:fillStatusBar="false"
        app:titleBarHeight="45dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:showBottomLine="false"
        app:centerType="textView"
        app:centerTextColor="#333333"
        app:centerTextSize="18sp"
        app:rightType="imageButton"
       app:rightImageResource="@drawable/main_search_black"
        />
    <include layout="@layout/public_mu_resh_listview"/>
</LinearLayout>